<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/Hinni/mystrom-api-csharp</PackageProjectUrl>
    <Product>myStrom API CSharp</Product>
    <Company>Hinni</Company>
    <Authors>Hinni</Authors>
    <Description>Inofficial myStrom API C# implementation</Description>
    <PackageTags>mystrom api</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/Hinni/mystrom-api-csharp/master/assets/icon-128x128.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/Hinni/mystrom-api-csharp.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Hinni/mystrom-api-csharp/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright Â© 2019 Hinni</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
